    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: debug exporter for ls",
            "type": "python",
            "request": "launch",
            "program": "./notion_exporter.py",
            "args": ["samples/sample.json"],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: exporter",
            "type": "python",
            "request": "launch",
            "program": "./notion_exporter.py",
            "args": ["${file}"],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}